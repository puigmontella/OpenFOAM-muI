FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     sedFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         4;

deltaT          1e-7;

writeControl    adjustableRunTime;

writeInterval   0.1;

purgeWrite      0;

writeFormat     binary;

writePrecision  7;

timeFormat      general;

timePrecision   7;

runTimeModifiable on;

adjustTimeStep  on;

maxCo           0.01;

maxAlphaCo      0.01;

maxDeltaT       1e-4;
#include "probe"


libs
(
    "libviscositymuI.so"
);
